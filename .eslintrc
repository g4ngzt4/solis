{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": 0,
    "no-multi-spaces": 0,
    "no-unused-expressions": 0,
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "import/no-extraneous-dependencies": ["warn", {"devDependencies": true, "optionalDependencies": false, "peerDependencies": false}],
    "prettier/prettier": [
      "error",
      {
        "printWidth": 160,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true
      }
    ]
  }
}